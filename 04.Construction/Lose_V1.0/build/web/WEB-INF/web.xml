<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>Main</servlet-name>
        <servlet-class>com.loser.map.Main</servlet-class>
        <load-on-startup>0</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>LocationGetByAll</servlet-name>
        <servlet-class>com.loser.map.services.mapview.LocationGetByAll</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetClusters</servlet-name>
        <servlet-class>com.loser.map.services.mapview.GetClusters</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>home</servlet-name>
        <jsp-file>/index.jsp</jsp-file>
        <init-param>
            <param-name>c</param-name>
            <param-value>maps</param-value>
        </init-param>
        <init-param>
            <param-name>f</param-name>
            <param-value>index</param-value>
        </init-param>
    </servlet>
    <servlet>
        <servlet-name>maps</servlet-name>
        <jsp-file>/index.jsp</jsp-file>
        <init-param>
            <param-name>c</param-name>
            <param-value>maps</param-value>
        </init-param>
        <init-param>
            <param-name>f</param-name>
            <param-value>index</param-value>
        </init-param>
    </servlet>
    <servlet-mapping>
        <servlet-name>Main</servlet-name>
        <url-pattern>/Main</url-pattern>
        <load-on-startup>0</load-on-startup>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LocationGetByAll</servlet-name>
        <url-pattern>/LocationGetByAll</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetClusters</servlet-name>
        <url-pattern>/GetClusters</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>home</servlet-name>
        <url-pattern>/home</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>maps</servlet-name>
        <url-pattern>/maps</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
